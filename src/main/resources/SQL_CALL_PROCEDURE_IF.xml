<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CALL_PROCEDURE_IF_TEST">

    <select id="SELECT" resultType="java.util.HashMap">
        SELECT * FROM ifs_hosiptal_info WHERE encryption_symbol='JDQ4MTYyMiM3MSMkMSMkMCMkNzIkMzgxOTYxIzExIyQxIyQzIyQ4OSQyNjEwMDIjNjEjJDEjJDAjJDgz'<!--esb_link_stcd_mid != 'L' LIMIT 1-->
    </select>

    <select id="CALL_NO_RETURN" resultType="java.util.HashMap">
        CALL public.test_proc_1(#{encryption_symbol}, 'Z')
    </select>

    <select id="CALL_RETURN_1" resultType="java.util.HashMap">
        CALL public.test_proc_1_return(
            #{encryption_symbol},
            'H',
            null)
    </select>

    <select id="CALL_RETURN_A_ROW" resultType="java.util.HashMap">
            --BEGIN;
            CALL public.test_proc_1_return_table(
                #{encryption_symbol},
                'K',
                'result')
    </select>

    <select id="OPEN_CURSOR">
        declare row_cursor cursor for select * from ifs_hosiptal_info;
    </select>

    <select id="FETCH" resultType="java.util.HashMap">
        FETCH ALL FROM result
    </select>

    <select id="CALL_RETURN_MULTI_ROWS" resultType="java.util.HashMap">
        BEGIN;
        CALL public.test_proc_1_return_rows(
            'N',
            10,
            'records')
    </select>

</mapper>
